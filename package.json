    "@tiptap/extension-highlight": "^2.10.5",
    "@tiptap/extension-image": "^2.10.5",
    "@tiptap/extension-link": "^2.10.5",
    "@tiptap/extension-placeholder": "^2.10.5",
    "@tiptap/extension-table": "^2.10.5",
    "@tiptap/extension-table-cell": "^2.10.5",
    "@tiptap/extension-table-header": "^2.10.5",
    "@tiptap/extension-table-row": "^2.10.5",
    "@tiptap/extension-task-item": "^2.26.4",
    "@tiptap/extension-task-list": "^2.26.4",
    "@tiptap/extension-text-style": "^2.10.5",
    "@tiptap/extension-typography": "^2.26.4",
    "@tiptap/extension-youtube": "^2.10.5",
    "@tiptap/html": "^2.26.4",
    "@tiptap/react": "^2.10.5",
    "@tiptap/starter-kit": "^2.10.5",
    "@types/ioredis": "^4.28.10",
    "@uiw/react-md-editor": "^4.0.8",
    "better-auth": "^1.3.25",
    "bullmq": "^5.61.0",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.7",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "html-to-text": "^9.0.5",
    "ioredis": "^5.8.0",
    "lowlight": "^3.1.0",
    "lucide-react": "^0.548.0",
    "next": "15.5.4",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
