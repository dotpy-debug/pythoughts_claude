{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:next": "next dev --turbo=false",
    "dev:collab": "tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:collab\"",
    "build": "vite build",
    "build:next": "next build",
    "build:all": "npm run build && npm run build:next",
    "build:prod": "npm run seo:generate && npm run build:all",
    "seo:generate": "tsx scripts/generate-seo-files.ts",
    "start": "tsx scripts/start-with-migrations.ts",
    "start:next": "next start",
    "start:collab": "tsx server/index.ts",
    "start:all": "concurrently \"npm run start:next\" \"npm run start:collab\"",
    "lint": "eslint .",
    "lint:next": "next lint",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:integration": "vitest run src/tests/integration",
    "test:integration:watch": "vitest watch src/tests/integration",
    "test:all": "vitest run && playwright test",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "coverage:view": "node -e \"const { exec } = require('child_process'); const opener = process.platform === 'win32' ? 'start' : process.platform === 'darwin' ? 'open' : 'xdg-open'; exec(`${opener} coverage/index.html`);\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "playwright:install": "playwright install --with-deps",
    "migrate": "node scripts/run-migrations.mjs",
    "migrate:status": "node scripts/migration-status.mjs",
    "db:migrate": "tsx scripts/migrate-database.ts",
    "db:migrate:production": "tsx scripts/migrate-database-production.ts",
    "db:push": "drizzle-kit push:pg",
    "db:generate": "drizzle-kit generate:pg",
    "db:studio": "drizzle-kit studio",
    "db:drop": "drizzle-kit drop",
    "db:sync": "tsx scripts/drizzle-schema-sync.ts",
    "db:sync:production": "tsx scripts/drizzle-schema-sync.ts --production",
    "db:sync:check": "tsx scripts/drizzle-schema-sync.ts --check",
    "db:auto-push": "tsx scripts/drizzle-auto-push.ts",
    "migration:run": "tsx scripts/migration-manager.ts",
    "migration:dry-run": "tsx scripts/migration-manager.ts --dry-run",
    "migration:validate": "tsx scripts/migration-manager.ts --validate-only",
    "migration:status": "tsx scripts/migration-manager.ts --status",
    "migration:rollback": "tsx scripts/migration-manager.ts --rollback",
    "migration:test": "tsx scripts/migration-tester.ts",
    "health:check": "tsx scripts/health-check.ts",
    "deploy": "tsx scripts/automated-deployment.ts",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "docker:build": "docker build -t pythoughts:latest .",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "prepare": "husky",
    "bolt:fix": "bash scripts/bolt-fix.sh",
    "bolt:dev": "NEXT_PUBLIC_DISABLE_IMAGE_OPTIMIZATION=true next dev --turbo=false",
    "bolt:build": "NEXT_PUBLIC_DISABLE_IMAGE_OPTIMIZATION=true next build",
    "bolt:start": "NEXT_PUBLIC_DISABLE_IMAGE_OPTIMIZATION=true next start",
    "hooks:analyze": "tsx .githooks/scripts/analyze-changes.ts",
    "hooks:test": "echo 'Testing hooks...' && tsx .githooks/scripts/pre-commit-check.ts || true",
    "hooks:clear-cache": "rm -rf .githooks/.cache",
    "hooks:clear-reports": "rm -rf .githooks/reports"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
